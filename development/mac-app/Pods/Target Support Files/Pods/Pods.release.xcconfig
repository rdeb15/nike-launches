FRAMEWORK_SEARCH_PATHS = "${PODS_ROOT}/Sparkle" "$(PODS_ROOT)/Sparkle"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/ISO8601DateFormatter" "${PODS_ROOT}/Headers/Public/MASShortcut" "${PODS_ROOT}/Headers/Public/RHPreferences" "${PODS_ROOT}/Headers/Public/Sparkle"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/ISO8601DateFormatter" -isystem "${PODS_ROOT}/Headers/Public/MASShortcut" -isystem "${PODS_ROOT}/Headers/Public/RHPreferences" -isystem "${PODS_ROOT}/Headers/Public/Sparkle"
OTHER_LDFLAGS = -ObjC -l"Pods-ISO8601DateFormatter" -l"Pods-MASShortcut" -l"Pods-RHPreferences" -l"Pods-Sparkle" -framework "Carbon" -framework "Cocoa" -framework "Sparkle"
OTHER_LIBTOOLFLAGS = $(OTHER_LDFLAGS)
PODS_ROOT = ${SRCROOT}/Pods